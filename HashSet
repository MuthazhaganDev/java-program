package set;
import java.util.HashSet;
public class Set {
public static HashSet<Integer> Creation () {
    HashSet<Integer> set = new HashSet<>();
    set.add(1);
    set.add(2);
    set.add(3);
    set.add(4);
//    System.out.println(set);
    return set;
}
    public static  void main (String [] muthu) {
        HashSet<Integer> set = Creation();
        System.out.println("The Set " + set);
        System.out.println("The Remove element : "+set.remove(4));
        System.out.println("After removed : " + set);
        boolean a = set.contains(2);
        System.out.println("The Searched element "+a );
        System.out.println("THe size of set : "+ set.size());
        System.out.println(set.isEmpty());
        set.clear();
        System.out.println(set);
    }
}
